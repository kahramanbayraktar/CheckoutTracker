@using Customer.Domain.Models;
@model List<CustomerViewModel>
@{
    ViewData["Title"] = "Home Page";
}

<h2>CUSTOMERS</h2>

<table>
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
    </tr>
    @foreach (var customer in Model)
    {
        <tr>
            <td>@customer.FirstName</td>
            <td>@customer.LastName</td>
        </tr>
    }
</table>